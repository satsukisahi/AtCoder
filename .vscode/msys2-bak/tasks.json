{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "process",
      "label": "gcc build active file",
      "command": "g++",
      "args": [
        "-g",
        "-O0",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}.exe"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}