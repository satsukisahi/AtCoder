//配列の中身を見る

for (int i = 0; i < n; ++i) {
    cout << table[i] << " ";
}cout  << endl;

//vectorの中身を見る

for (int i = 0; i < v.size(); ++i) {
    cout << v[i] << " ";    
}cout  << endl;

for (auto& u : v) {
    cout << u << " ";
}cout  << endl;

//2次元配列の中身を見る

for (int i = 0; i < n; ++i){
for (int j = 0; j < m; ++j) {
    cout << table[i][j] << " ";
}cout  << endl;}